
@model ToDoAppMVC.ViewModels.UsersAndTasksVM

@{
    ViewBag.Title = "Index";
}

<h2>To Do App</h2>

<table>
    <thead>
        <tr>
        <th>ID</th>
        <th>Creator Name</th>
        <th>Task Name</th>
        <th>Task Description</th>
        <th>Completed</th>
        </tr>
    </thead>
    <tbody>
        
        @foreach (var task in Model.Tasks)
        {
        <tr>
            <td>@task.Id</td>
        </tr>
        }
    </tbody>
</table>